def getAvailableLetters(lettersGuessed):
    '''
    lettersGuessed: list, what letters have been guessed so far
    returns: string, comprised of letters that represents what letters have not
      yet been guessed.
    '''
    # FILL IN YOUR CODE HERE...
    lettersnotguessed = ['a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's', 't', 'u', 'v', 'w', 'x', 'y', 'z']
    for i in range (0, len(lettersGuessed)):
        if lettersGuessed[i] in lettersnotguessed:
            lettersnotguessed.remove(lettersGuessed[i])
    lettersnotguessed = ' '.join(lettersnotguessed)
    return lettersnotguessed
